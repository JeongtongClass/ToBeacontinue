<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main">

    <ImageButton
        android:id="@+id/houseButton"
        android:layout_width="127dp"
        android:layout_height="136dp"
        android:layout_marginBottom="309dp"
        android:scaleType="fitXY"
        android:background="@color/backgroundColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/outsideButton"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.423"
        app:srcCompat="@mipmap/home_button" />

    <ImageButton
        android:id="@+id/outsideButton"
        android:layout_width="127dp"
        android:layout_height="136dp"
        android:background="@color/backgroundColor"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/cafeteriaButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.788"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.87"
        app:srcCompat="@mipmap/outside_button" />

    <ImageButton
        android:id="@+id/schoolButton"
        android:layout_width="127dp"
        android:layout_height="136dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        android:background="@color/backgroundColor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/houseButton"
        app:srcCompat="@mipmap/school_button" />

    <ImageButton
        android:id="@+id/cafeteriaButton"
        android:layout_width="127dp"
        android:layout_height="136dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:background="@color/backgroundColor"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.016"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458"
        app:srcCompat="@mipmap/cafeteria_button"
        app:layout_constraintStart_toEndOf="@+id/schoolButton"
        app:layout_constraintTop_toBottomOf="@+id/outsideButton"/>

    <ImageButton
        android:id="@+id/addButton"
        android:layout_width="253dp"
        android:layout_height="63dp"
        android:scaleType="fitXY"
        android:background="@color/backgroundColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.914"
        app:srcCompat="@mipmap/addlocation_button" />
</androidx.constraintlayout.widget.ConstraintLayout>